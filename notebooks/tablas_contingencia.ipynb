    "crear_tabla_por_factor <- function(factor_var, valores_factor) {\n",
    "  \n",
    "  perfiles <- expand.grid(\n",
    "    Modelo = levels(datos$Modelo)[1],\n",
    "    Edad = levels(datos$Edad)[1], \n",
    "    Color = if(factor_var == \"Color\") valores_factor else levels(datos$Color)[1],\n",
    "    Carroceria = if(factor_var == \"Carroceria\") valores_factor else levels(datos$Carroceria)[1],\n",
    "    SERVICIO = if(factor_var == \"SERVICIO\") valores_factor else levels(datos$SERVICIO)[1],\n",
    "    stringsAsFactors = FALSE\n",
    "  )\n",
    "  \n",
    "  resultados <- data.frame()\n",
    "  \n",
    "  for (i in 1:nrow(perfiles)) {\n",
    "    perfil <- perfiles[i, ]\n",
    "    resultado <- calcular_prima_pura(perfil$Modelo, perfil$Edad, perfil$Color, \n",
    "                                   perfil$Carroceria, perfil$SERVICIO)\n",
    "    \n",
    "    if (resultado$exito) {\n",
    "      factor_value <- switch(factor_var,\n",
    "                           \"Color\" = perfil$Color,\n",
    "                           \"Carroceria\" = perfil$Carroceria, \n",
    "                           \"SERVICIO\" = perfil$SERVICIO)\n",
    "      \n",
    "      resultados <- rbind(resultados, data.frame(\n",
    "        Factor = factor_value,\n",
    "        Frecuencia = round(resultado$frecuencia, 6),\n",
    "        Severidad = round(resultado$severidad, 0),\n",
    "        Prima_Pura = round(resultado$prima, 0),\n",
    "        stringsAsFactors = FALSE\n",
    "      ))\n",
    "    }\n",
    "  }\n",
    "  \n",
    "  if (nrow(resultados) > 0) {\n",
    "    resultados$Rel_Frecuencia <- round(resultados$Frecuencia / resultados$Frecuencia[1], 3)\n",
    "    resultados$Rel_Severidad <- round(resultados$Severidad / resultados$Severidad[1], 3)\n",
    "    resultados$Rel_Prima <- round(resultados$Prima_Pura / resultados$Prima_Pura[1], 3)\n",
    "  }\n",
    "  \n",
    "  return(resultados)\n",
    "}\n",
    "\n",
    "cat(\"\\n\")\n",
    "cat(\"═══════════════════════════════════════════════════════════════════\\n\")\n",
    "cat(\"                       ANÁLISIS POR COLOR                          \\n\")\n",
    "cat(\"═══════════════════════════════════════════════════════════════════\\n\")\n",
    "tabla_color <- crear_tabla_por_factor(\"Color\", levels(datos$Color))\n",
    "print(tabla_color, row.names = FALSE)\n",
    "\n",
    "cat(\"\\n\")\n",
    "cat(\"═══════════════════════════════════════════════════════════════════\\n\")\n",
    "cat(\"                    ANÁLISIS POR CARROCERÍA                        \\n\")\n",
    "cat(\"═══════════════════════════════════════════════════════════════════\\n\")\n",
    "tabla_carroceria <- crear_tabla_por_factor(\"Carroceria\", levels(datos$Carroceria))\n",
    "print(tabla_carroceria, row.names = FALSE)\n",
    "\n",
    "cat(\"\\n\")\n",
    "cat(\"═══════════════════════════════════════════════════════════════════\\n\")\n",
    "cat(\"                     ANÁLISIS POR SERVICIO                         \\n\")\n",
    "cat(\"═══════════════════════════════════════════════════════════════════\\n\")\n",
    "tabla_servicio <- crear_tabla_por_factor(\"SERVICIO\", levels(datos$SERVICIO))\n",
    "print(tabla_servicio, row.names = FALSE)\n",
    "\n",
    "crear_resumen_ejecutivo <- function() {\n",
    "  base_freq <- tabla_contingencia$Frecuencia[1]\n",
    "  base_sev <- tabla_contingencia$Severidad[1] \n",
    "  base_prima <- tabla_contingencia$Prima_Pura[1]\n",
    "  \n",
    "  factor_mayor_impacto_freq <- tabla_color$Factor[which.max(tabla_color$Rel_Frecuencia)]\n",
    "  factor_mayor_impacto_sev <- tabla_servicio$Factor[which.max(tabla_servicio$Rel_Severidad)]\n",
    "  factor_mayor_impacto_prima <- tabla_servicio$Factor[which.max(tabla_servicio$Rel_Prima)]\n",
    "  \n",
    "  resumen <- data.frame(\n",
    "    Metrica = c(\"Prima Base (COP)\", \"Factor Mayor Impacto Frecuencia\", \n",
    "               \"Factor Mayor Impacto Severidad\", \"Factor Mayor Impacto Prima\",\n",
    "               \"Rango Frecuencia\", \"Rango Severidad\", \"Rango Prima\"),\n",
    "    Valor = c(\n",
    "      format(round(base_prima), big.mark = \",\"),\n",
    "      paste(factor_mayor_impacto_freq, \"- Color\"),\n",
    "      paste(factor_mayor_impacto_sev, \"- Servicio\"), \n",
    "      paste(factor_mayor_impacto_prima, \"- Servicio\"),\n",
    "      paste(round(min(tabla_contingencia$Frecuencia), 6), \"-\", \n",
    "            round(max(tabla_contingencia$Frecuencia), 6)),\n",
    "      paste(format(round(min(tabla_contingencia$Severidad)), big.mark = \",\"), \"-\",\n",
    "            format(round(max(tabla_contingencia$Severidad)), big.mark = \",\")),\n",
    "      paste(format(round(min(tabla_contingencia$Prima_Pura)), big.mark = \",\"), \"-\",\n",
    "            format(round(max(tabla_contingencia$Prima_Pura)), big.mark = \",\"))\n",
    "    ),\n",
    "    stringsAsFactors = FALSE\n",
    "  )\n",
    "  \n",
    "  return(resumen)\n",
    "}\n",
    "\n",
    "cat(\"\\n\")\n",
    "cat(\"═══════════════════════════════════════════════════════════════════\\n\")\n",
    "cat(\"                       RESUMEN EJECUTIVO                           \\n\")\n",
    "cat(\"═══════════════════════════════════════════════════════════════════\\n\")\n",
    "resumen_ejecutivo <- crear_resumen_ejecutivo()\n",
    "print(resumen_ejecutivo, row.names = FALSE)"
