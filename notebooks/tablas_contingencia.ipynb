    "calcular_relatividades <- function(coeficientes, nombre_factor, niveles_factor) {\n",
    "  indices_factor <- grep(paste0(\"^\", nombre_factor), names(coeficientes))\n",
    "  \n",
    "  if (length(indices_factor) == 0) {\n",
    "    return(rep(1, length(niveles_factor)))\n",
    "  }\n",
    "  \n",
    "  relatividades_no_ref <- exp(coeficientes[indices_factor])\n",
    "  relatividades <- rep(1, length(niveles_factor))\n",
    "  \n",
    "  if (length(relatividades_no_ref) == length(niveles_factor) - 1) {\n",
    "    relatividades[2:length(niveles_factor)] <- relatividades_no_ref\n",
    "  } else if (length(relatividades_no_ref) < length(niveles_factor) - 1) {\n",
    "    relatividades[2:(length(relatividades_no_ref) + 1)] <- relatividades_no_ref\n",
    "  }\n",
    "  \n",
    "  return(relatividades)\n",
    "}\n",
    "\n",
    "construir_tabla_relatividades <- function() {\n",
    "  tabla_relatividades <- data.frame()\n",
    "  vars_todas <- unique(c(vars_frecuencia, vars_severidad))\n",
    "  \n",
    "  for (factor in vars_todas) {\n",
    "    if (factor %in% colnames(datos) && is.factor(datos[[factor]])) {\n",
    "      niveles <- levels(datos[[factor]])\n",
    "      \n",
    "      if (factor %in% vars_frecuencia) {\n",
    "        rel_freq <- calcular_relatividades(beta_freq, factor, niveles)\n",
    "      } else {\n",
    "        rel_freq <- rep(1, length(niveles))\n",
    "      }\n",
    "      \n",
    "      if (factor %in% vars_severidad) {\n",
    "        rel_sev <- calcular_relatividades(beta_sev, factor, niveles)\n",
    "      } else {\n",
    "        rel_sev <- rep(1, length(niveles))\n",
    "      }\n",
    "      \n",
    "      rel_prima <- rel_freq * rel_sev\n",
    "      \n",
    "      if (length(niveles) > 0 && length(rel_freq) == length(niveles) && length(rel_sev) == length(niveles)) {\n",
    "        temp_df <- data.frame(\n",
    "          Factor = rep(factor, length(niveles)),\n",
    "          Nivel = niveles,\n",
    "          Rel_Frecuencia = round(rel_freq, 3),\n",
    "          Rel_Severidad = round(rel_sev, 3),\n",
    "          Rel_Prima = round(rel_prima, 3),\n",
    "          stringsAsFactors = FALSE\n",
    "        )\n",
    "        \n",
    "        tabla_relatividades <- rbind(tabla_relatividades, temp_df)\n",
    "      }\n",
    "    }\n",
    "  }\n",
    "  \n",
    "  return(tabla_relatividades)\n",
    "}\n",
    "\n",
    "tabla_relatividades <- construir_tabla_relatividades()\n",
    "\n",
    "cat(\"═══════════════════════════════════════════════════════════════════\\n\")\n",
    "cat(\"                    TABLA DE RELATIVIDADES POR FACTOR              \\n\")\n",
    "cat(\"═══════════════════════════════════════════════════════════════════\\n\")\n",
    "print(tabla_relatividades, row.names = FALSE)"
   ]
