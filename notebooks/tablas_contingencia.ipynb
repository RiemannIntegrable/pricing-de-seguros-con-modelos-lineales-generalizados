    "calcular_prima_pura <- function(modelo_val, edad_val, color_val, carroceria_val, servicio_val, exposicion = 1) {\n",
    "  \n",
    "  perfil_freq <- data.frame(\n",
    "    Modelo = factor(modelo_val, levels = levels(datos$Modelo)),\n",
    "    Edad = factor(edad_val, levels = levels(datos$Edad)),\n",
    "    exposicion_total = exposicion\n",
    "  )\n",
    "  \n",
    "  perfil_sev <- data.frame(\n",
    "    Modelo = factor(modelo_val, levels = levels(datos$Modelo)),\n",
    "    Color = factor(color_val, levels = levels(datos$Color)),\n",
    "    Carroceria = factor(carroceria_val, levels = levels(datos$Carroceria)),\n",
    "    SERVICIO = factor(servicio_val, levels = levels(datos$SERVICIO)),\n",
    "    Edad = factor(edad_val, levels = levels(datos$Edad)),\n",
    "    n_siniestros = 1\n",
    "  )\n",
    "  \n",
    "  tryCatch({\n",
    "    freq_pred <- predict(modelo_frecuencia, newdata = perfil_freq, type = \"response\")\n",
    "    tasa_frecuencia <- freq_pred / exposicion\n",
    "    \n",
    "    sev_pred_log <- predict(modelo_severidad, newdata = perfil_sev, type = \"response\")\n",
    "    \n",
    "    similar_data <- datos[\n",
    "      datos$Modelo == modelo_val & \n",
    "      datos$Color == color_val & \n",
    "      datos$Carroceria == carroceria_val & \n",
    "      datos$SERVICIO == servicio_val & \n",
    "      datos$Edad == edad_val, \n",
    "    ]\n",
    "    \n",
    "    if (nrow(similar_data) > 0) {\n",
    "      severidad_promedio <- mean(similar_data$severidad_media, na.rm = TRUE)\n",
    "    } else {\n",
    "      suma_pagos_pred <- exp(sev_pred_log)\n",
    "      n_siniestros_promedio <- mean(datos[datos$SERVICIO == servicio_val, ]$n_siniestros, na.rm = TRUE)\n",
    "      if (is.na(n_siniestros_promedio)) n_siniestros_promedio <- mean(datos$n_siniestros)\n",
    "      severidad_promedio <- suma_pagos_pred / n_siniestros_promedio\n",
    "    }\n",
    "    \n",
    "    prima_pura <- tasa_frecuencia * severidad_promedio\n",
    "    \n",
    "    return(list(\n",
    "      frecuencia = as.numeric(tasa_frecuencia),\n",
    "      severidad = as.numeric(severidad_promedio),\n",
    "      prima = as.numeric(prima_pura),\n",
    "      exito = TRUE\n",
    "    ))\n",
    "  }, error = function(e) {\n",
    "    return(list(exito = FALSE, error = as.character(e)))\n",
    "  })\n",
    "}\n",
    "\n",
    "crear_tabla_contingencia_completa <- function() {\n",
    "  \n",
    "  perfiles_base <- expand.grid(\n",
    "    Modelo = levels(datos$Modelo),\n",
    "    Edad = levels(datos$Edad),\n",
    "    Color = levels(datos$Color)[1],\n",
    "    Carroceria = levels(datos$Carroceria)[1], \n",
    "    SERVICIO = levels(datos$SERVICIO)[1],\n",
    "    stringsAsFactors = FALSE\n",
    "  )\n",
    "  \n",
    "  resultados <- data.frame()\n",
    "  \n",
    "  for (i in 1:nrow(perfiles_base)) {\n",
    "    perfil <- perfiles_base[i, ]\n",
    "    resultado <- calcular_prima_pura(perfil$Modelo, perfil$Edad, perfil$Color, \n",
    "                                   perfil$Carroceria, perfil$SERVICIO)\n",
    "    \n",
    "    if (resultado$exito) {\n",
    "      resultados <- rbind(resultados, data.frame(\n",
    "        Modelo = perfil$Modelo,\n",
    "        Edad = perfil$Edad,\n",
    "        Frecuencia = resultado$frecuencia,\n",
    "        Severidad = resultado$severidad,\n",
    "        Prima_Pura = resultado$prima,\n",
    "        stringsAsFactors = FALSE\n",
    "      ))\n",
    "    }\n",
    "  }\n",
    "  \n",
    "  return(resultados)\n",
    "}\n",
    "\n",
    "tabla_contingencia <- crear_tabla_contingencia_completa()\n",
    "\n",
    "tabla_pivot_freq <- tabla_contingencia %>%\n",
    "  select(Modelo, Edad, Frecuencia) %>%\n",
    "  pivot_wider(names_from = Edad, values_from = Frecuencia) %>%\n",
    "  mutate_if(is.numeric, ~ round(.x, 6))\n",
    "\n",
    "tabla_pivot_sev <- tabla_contingencia %>%\n",
    "  select(Modelo, Edad, Severidad) %>%\n",
    "  pivot_wider(names_from = Edad, values_from = Severidad) %>%\n",
    "  mutate_if(is.numeric, ~ round(.x, 0))\n",
    "\n",
